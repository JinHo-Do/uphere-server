{
  "name": "uphere-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "mocha spec || NODE_ENV=test YOURPACKAGE_COVERAGE=1 ./node_modules/.bin/mocha spec --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "express": "^4.15.2"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "supertest": "^3.0.0"
  },
  "config": {
    "blanket": {
      "pattern": ["server.js", "src"],
      "data-cover-never": ["node_modules", "spec"]
    }
  }
}
